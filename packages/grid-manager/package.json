{
  "name": "lt-grid-manager",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "main": "dist/grid-manager.js",
  "module": "dist/grid-manager.module.js",
  "unpkg": "dist/grid-manager.umd.js",
  "scripts": {
    "build": "microbundle --external=none",
    "dev": "microbundle --external=none watch ",
    "test": "jest",
    "format": "prettier --write \"**/*.ts{,on}\" \"**/*.md\"  \"**/*.mdx\"",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "serve": "cp index.html dist/ && yarn http-server ./dist -p 9000"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    },
    "lint-staged": {
      "*.{js,ts,tsx}": [
          "eslint --fix"
      ]
    }
  },
  "dependencies": {
    "lodash": "^4.17.20",
    "xstate": "^4.13.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint": "5.15.1",
    "eslint-config-developit": "^1.1.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "html-webpack-plugin": "^4.4.1",
    "http-server": "^0.12.3",
    "husky": "^3.0.9",
    "microbundle": "^0.12.3",
    "prettier": "^2.1.2",
    "typescript": "3.5.3"
  }
}
