{
  "name": "@lets-talk/lt-grid-manager",
  "version": "0.2.3-alpha.0",
  "license": "MIT",
  "private": false,
  "main": "dist/grid-manager.js",
  "module": "dist/grid-manager.module.js",
  "unpkg": "dist/grid-manager.umd.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "microbundle --external=none",
    "install-isolate": "yarn --pure-lockfile",
    "build": "microbundle --external=none",
    "dev": "microbundle --external=none watch ",
    "test": "jest",
    "test:ci": "jest",
    "test-watch": "jest --watch",
    "format": "prettier --write \"**/*.ts{,on}\" \"**/*.md\"  \"**/*.mdx\"",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "serve": "parcel index.html -p 9000"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    },
    "lint-staged": {
      "*.{js,ts,tsx}": [
        "eslint --fix"
      ]
    }
  },
  "dependencies": {
    "lodash": "^4.17.20",
    "xstate": "^4.13.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.16",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint": "5.15.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "html-webpack-plugin": "^4.4.1",
    "http-server": "^0.12.3",
    "husky": "^3.0.9",
    "jest": "^26.4.4",
    "microbundle": "^0.12.4",
    "parcel": "^2.0.0-rc.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.4"
  },
  "gitHead": "c28c3c03fd99c8417eabe0defc2e33a190d3a96f"
}
